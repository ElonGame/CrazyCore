<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <AssetDir>$(MSBuildThisFileDirectory)../Assets</AssetDir>
    <ProjectDir>$(MSBuildThisFileDirectory)</ProjectDir>
    <BinDir>$(ProjectDir)\bin</BinDir>
    <ObjDir>$(BinDir)\obj</ObjDir>
    <OutputPath>$(BinDir)\$(Configuration)\$(MSBuildProjectName)</OutputPath>
    <BaseIntermediateOutputPath>$(ObjDir)\$(Configuration)\$(MSBuildProjectName)</BaseIntermediateOutputPath>
  </PropertyGroup>
</Project>
